  procedures :
   -
     name : OnGesture
     procedure_id : 1965191393307112313
     type_code : 14
     code : |1+
      // Résumé : Evénement WM_GESTURE
      // Syntaxe :
      //[ <Résultat> = ] OnGesture ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Type indéterminé : 
      //
      PROCEDURE OnGesture()
      
      	nHandle 		est un entier systeme = _eve.lparam
      	stGesture 		est un gestureinfo
      	bGéré			est un booléen
      	rRatio			est un reel
      	rAngle			est un reel
      		
      	// Taille de la structure
      	stGesture.cbSize = sizeofGESTUREINFO
      		
      	// Récupère les informations
      	si GetGestureInfo(nHandle, &stGesture) alors
      			
      		// En fonction du type de mouvement reçu
      		selon stGesture.dwID
      						
      			// Windows a détecté un mouvement de zoom
      			cas gid_zoom		
      			
      				bGéré = vrai
      				
      				// Champ image
      				si stGesture.hwndTarget = handle(IMG_DESSIN) alors
      				
      					// Si le mouvement démarre
      					SI stGesture.dwFlags = GF_BEGIN ALORS
      						
      						// Conserve le membre ullArguments
      						IMG_DESSIN..note 	= LODWORD(stGesture.ullArguments)		
      						IMG_DESSIN = IMG_DESSIN
      						
      					sinon
      						
      						// Calcule le facteur du zoom
      						rRatio = LODWORD(stGesture.ullArguments) / val(IMG_DESSIN..Note)
      						
      						// Conserve le membre ullArguments
      						IMG_DESSIN..Note = LODWORD(stGesture.ullArguments)		
      												
      						// Applique le zoom
      						IMG_DESSIN..Zoom = IMG_DESSIN..Zoom * rRatio
      						IMG_DESSIN..ImageMode = imgCentré
      						Temporisation(0, tempoDessin)
      					FIN
      					
      				sinon si stGesture.hwndTarget = handle(TABLE_Table) alors
      					
      					// Si le mouvement démarre
      					SI stGesture.dwFlags = GF_BEGIN ALORS
      						
      						// Conserve le membre ullArguments
      						TABLE_Table..Note 	= LODWORD(stGesture.ullArguments)		
      						
      					SINON
      						
      						// Calcule le facteur du zoom
      						rRatio = LODWORD(stGesture.ullArguments) / Val(TABLE_Table..Note)
      						
      						// Conserve le membre ullArguments
      						TABLE_Table..Note = LODWORD(stGesture.ullArguments)		
      						
      						// Applique le zoom
      						TABLE_Table..HauteurLigne = TABLE_Table..HauteurLigne * rRatio
      						TABLE_Table..HauteurLigne = max(8, TABLE_Table..HauteurLigne)
      						Temporisation(0, tempoDessin)
      					FIN
      					
      				fin				
      							
      			// Windows a détecté une rotation
      			cas GID_ROTATE			
      				
      				bGéré = Vrai				
      				
      				// Champ image
      				SI stGesture.hwndTarget = Handle(IMG_DESSIN) ALORS
      									
      					// Si le mouvement démarre
      					si pas stGesture.dwFlags = GF_BEGIN ALORS
      											
      						rAngle = GID_ROTATE_ANGLE_FROM_ARGUMENT(LODWORD(stGesture.ullArguments))
      						rAngle = rAngle * 180 / valpi
      					
      						IMG_DESSIN..AffichageActif = faux
      						IMG_DESSIN = IMG_DESSIN
      						dRotation(IMG_DESSIN, -rAngle)
      						IMG_DESSIN..AffichageActif = vrai
      						
      						IMG_DESSIN..ImageMode = imgCentré
      						Temporisation(0, tempoDessin)				
      					FIN		
      				sinon SI stGesture.hwndTarget = Handle(TABLE_Table) ALORS
      						
      					// Si le mouvement démarre
      					SI PAS stGesture.dwFlags = GF_BEGIN ALORS
      						
      						rAngle = GID_ROTATE_ANGLE_FROM_ARGUMENT(LODWORD(stGesture.ullArguments))
      						rAngle = rAngle * 180 / ValPI
      						rAngle = rAngle * 90 / 360
      																		
      						si rAngle < 0 alors
      							TABLE_Table..Vertical = Vrai
      							TableAjuste(TABLE_Table)
      						sinon
      							TABLE_Table..Vertical = faux
      							TableAjuste(TABLE_Table)
      						FIN
      						
      						Temporisation(0, tempoDessin)				
      					FIN	
      				fin					
      				
      		FIN
      	FIN
      		
      	// Pour les autres mouvements, on doit renvoyer le résultat de l'appel à DefWindowProc
      	si bGéré = faux ALORS		
      		renvoyer api(user32, "DefWindowProcA", _eve.hwnd, _eve.wmessage, _eve.wparam, _eve.lparam)
      	FIN
      		
      	// Pour les autres cas, on doit libérer le handle
      	CloseGestureInfoHandle(nHandle)
      	
      renvoyer vrai
     type : 458752
   -
     name : OnGestureConfig
     procedure_id : 1965191393307177849
     type_code : 14
     code : |1-
      // Résumé : Evénément WM_GESTURENOTIFY
      // Syntaxe :
      //[ <Résultat> = ] OnGestureConfig ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	variant : // 	Aucune
      //
      PROCEDURE OnGestureConfig()
      
      	stConfig est un gestureconfig
      	stNotify est un GESTURENOTIFYSTRUCT
      	nHandle  est un entier systeme
      	
      	// Récupère les données
      	transfert(&stNotify, _eve.lparam, dimension(stNotify))
      	
      	// Récupère le handle pour lequel on doit configurer les mouvements
      	nHandle = stNotify.hwndTarget
      	
      	// On accepte tous les mouvements
      	stConfig = [0, GC_ALLGESTURES, 0]
      	
      	// Applique la configuration
      	SetGestureConfig(nHandle, 0, 1, &stConfig, sizeofGESTURECONFIG)
      		
      // On doit faire suivre au traitement initial
      RENVOYER API(USER32, "DefWindowProcA", _EVE.hWnd, _EVE.wMessage, _EVE.wParam, _EVE.lParam)
     type : 458752
